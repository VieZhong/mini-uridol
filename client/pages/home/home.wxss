 .container{
     width: 100%;
     height: 100%;
     background-size: 750rpx 1334rpx;
     background-position: center;
 }
 .container__who-img{
     width: 439rpx;
     height: 65rpx;
     display: block;
     margin: 80rpx auto;
 }
 .title {
     text-align: center;
     display: block;
     margin: 0 auto;
     line-height: 224rpx;
 }

 .bottom {
     width: 300rpx;
     position: absolute;
     bottom: 20rpx;
     left: 50%;
     margin-left: -150rpx;
 }

 .container__top-32 {
     margin-top: 32rpx;
 }
 
 .row__light{
     position: absolute;
     width: 152rpx;
     height: 168rpx;
     top:-2rpx;
     left:0;
     opacity: 0
 }
 .row__light_show{
     opacity: 1;
 }
 .container__row {
     width: 100%;
     display: -webkit-flex;
     display: flex;
     justify-content: center;
 }
 .row__item {
     flex-basis: 148rpx;
     height: 164rpx;
     position: relative;
 }
 .row__img{
     height:164rpx;
     width:148rpx; 
 }

 .item__col {
     width: 100%;
     display: -webkit-flex;
     display: flex;
     flex-direction: column;
 }
 .col__thing {
     width: 148rpx;
     height: 164rpx;
     position: relative;
 }
 .row__item-center {
     flex-basis: 338rpx;
     height: 360rpx;
 }
 .row__center-img{
     width: 338rpx;
     height: 360rpx;
 }

 .row__left {
     margin-left: 42rpx;
 }
 .container__match-img{
     display: block;
     width: 572rpx;
     height:94rpx;
     margin: 80rpx auto
 }